 entity line-continuation');\n  asciidoc['title'].inside.rest = copyFromAsciiDoc('macro inline replacement entity'); // Plugin to make entity title show the real entity, idea by Roman Komarov\n\n  Prism.hooks.add('wrap', function (env) {\n    if (env.type === 'entity') {\n      env.attributes['title'] = env.content.replace(/&amp;/, '&');\n    }\n  });\n  Prism.languages.adoc = Prism.languages.asciidoc;\n})(Prism);\n\n/***/ })\n\n}]);","inputSourceMap":{"version":3,"sources":["C:/Users/User/Desktop/jbook/node_modules/prismjs/components/prism-asciidoc.js"],"names":["Prism","attributes","pattern","lookbehind","inside","asciidoc","languages","alias","punctuation","copyFromAsciiDoc","keys","split","o","i","l","length","rest","hooks","add","env","type","content","replace","adoc"],"mappings":";;;;;AAAC,WAAUA,KAAV,EAAiB;AAEjB,MAAIC,UAAU,GAAG;AAChBC,WAAO,EAAE,0FADO;AAEhBC,cAAU,EAAE,IAFI;AAGhBC,UAAM,EAAE;AACP,gBAAU;AACTF,eAAO,EAAE,8BADA;AAETE,cAAM,EAAE;AACP,yBAAe;AADR;AAFC,OADH;AAOP,qBAAe;AACdF,eAAO,EAAE,mBADK;AAEdE,cAAM,EAAE;AACP,yBAAe,OADR,CAEP;;AAFO;AAFM,OAPR;AAcP,gBAAU,mBAdH;AAeP,kBAAY,UAfL;AAgBP,qBAAe,WAhBR;AAiBP,kBAAY,GAjBL;AAkBP;AACA,oBAAc;AAnBP;AAHQ,GAAjB;AA0BA,MAAIC,QAAQ,GAAGL,KAAK,CAACM,SAAN,CAAgBD,QAAhB,GAA2B;AACzC,qBAAiB;AAChBH,aAAO,EAAE,mDADO;AAEhBK,WAAK,EAAE;AAFS,KADwB;AAKzC,aAAS;AACRL,aAAO,EAAE,mDADD;AAERE,YAAM,EAAE;AACP,sBAAc;AACbF,iBAAO,EAAE,sFADI;AAEbK,eAAK,EAAE;AAFM,SADP;AAKP,uBAAe;AACdL,iBAAO,EAAE,iBADK;AAEdC,oBAAU,EAAE;AAFE,SALR,CASP;;AATO;AAFA,KALgC;AAoBzC,yBAAqB;AACpBD,aAAO,EAAE,oDADW;AAEpBE,YAAM,EAAE;AACP,uBAAe,WADR,CAEP;;AAFO;AAFY,KApBoB;AA2BzC;AACA,qBAAiB;AAChBF,aAAO,EAAE,0DADO;AAEhBE,YAAM,EAAE;AACP,uBAAe,yBADR,CAEP;;AAFO;AAFQ,KA5BwB;AAmCzC;AACA,mBAAe;AACdF,aAAO,EAAE,mEADK;AAEdE,YAAM,EAAE;AACP,uBAAe,qCADR,CAEP;;AAFO;AAFM,KApC0B;AA4CzC;AACA,wBAAoB;AACnBF,aAAO,EAAE,gEADU;AAEnBC,gBAAU,EAAE,IAFO;AAGnBI,WAAK,EAAE;AAHY,KA7CqB;AAkDzC,kBAAc;AACbL,aAAO,EAAE,yCADI;AAEbC,gBAAU,EAAE,IAFC;AAGbI,WAAK,EAAE;AAHM,KAlD2B;AAuDzC,sBAAkB;AACjBL,aAAO,EAAE,4DADQ;AAEjBC,gBA